{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": true,
    "title": "Bionic item",
    "type": "object",
    "id": "https://niebert.github.io/json-editor",
    "options": {
        "disable_collapse": false,
        "disable_edit_json": false,
        "disable_properties": false,
        "collapsed": false,
        "hidden": false
    },
    "defaultProperties": [
        "type",
        "id",
        "name"
    ],
    "properties": {
        "id": {
          "$ref": "#/definitions/id", 
          "propertyOrder": 10
        },
        "type": {
          "$ref": "#/definitions/type", 
          "propertyOrder": 11,
          "default": "BIONIC_ITEM"
        },
        "name": {
          "$ref": "#/definitions/name", 
          "propertyOrder": 12
        },
        "description": {
          "$ref": "#/definitions/description", 
          "propertyOrder": 12
        },
        "weight": {
          "$ref": "#/definitions/weight", 
          "propertyOrder": 13
        },
        "difficulty": {
            "type": "integer",
            "id": "/properties/difficulty",
            "title": "Difficulty",
            "default": 2,
            "description": "Difficulty of installing CBM",
            "options": {
                "hidden": false
            },
            "propertyOrder": 130
        },
        "copy-from": {
            "type": "string",
            "enum": [ ]
        }
    }
}